public class NestedLoops{
    
    public static void main(String[] args)
    {
        int a[][] = new int [12][13];
        int x;
        for (int i = 1; i <= 12; i++)
        {
            for (int j = 0; j <= 12; j++)
            {
                x = i*j;
                System.out.println(i + " * " + j + " = " + x);
            }
            System.out.println("*****");
        }
        
    }
}
