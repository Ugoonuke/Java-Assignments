public class UsingMod
{
    public static void main(String args [])
    {
        int num = 123;
        int num1 = num / 100;
        int num2 = num % 100;
        int num3 = num2 / 10;
        int num4 = num2 % 10;
        int num5 = num4 / 1;
        System.out.println("The digits in the number 123 are:");
        System.out.println(num1);
        System.out.println(num3);
        System.out.println(num5);
    }
}
