import java.util.Scanner;
public class input2
{
    public static void main(String args [])
    {
        Scanner scan = new Scanner(System.in);
        String a = scan.nextLine();
        if (a.charAt(0) == 'T' && a.charAt(a.length()-1) == 'd') System.out.println("This String starts with T and ends with d");
        else if (a.charAt(0) == 'T') System.out.println("This String starts with T");
        else if (a.charAt(a.length()-1) == 'd') System.out.println("This String ends with d");
        else System.out.println("This String DOES NOT start with T or end with d");
    }
}
