public class Factorial
{
    public static void main(String args [])
    {
        int num = 5;
        int num1 = 1;
        int count = num - 1;
        while (count >= 1)
        {
            num1 = num1 * count;
            count--;
        }
        num1 = num1 * num;
        System.out.println("The factorial of " + num + " is " + num1);
    }
}
